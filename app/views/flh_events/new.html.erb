<%= form_with(model: @flh_event, url: flh_events_path ) do |f| %>
    <div>
        <%= f.label :title %>
        <br>
        <%= f.text_field :title%>
    </div>
    <br>
    <div>
        <%= f.label :body %>
        <br>
        <%= f.text_area :body%>
    </div>
    <br>
    <div>
        <%= f.label :link %>
        <br>
        <%= f.text_field :link%>
    </div>
    <br>
    <div>
        <%= f.label :date %>
        <br>
        <%= f.date_field :date %>
    </div>
    <%= f.submit  "Submit" %>
<% end %>